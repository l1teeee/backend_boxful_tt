
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  nombre      String
  apellido    String
  sexo        String
  fechaNacimiento DateTime
  email       String   @unique
  telefono    String
  password    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("users")
}